{
  "name": "arkisto-cloud-aws",
  "version": "1.0.0",
  "bin": {
    "arkisto-cloud-aws": "bin/arkisto-cloud-aws.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "node_modules/.bin/cdk synth",
    "cdk:deploy": "node_modules/.bin/cdk deploy --require-approval any-change",
    "cdk:destroy": "node_modules/.bin/cdk destroy --require-approval any-change",
    "cdk:diff": "./node_modules/.bin/cdk diff",
    "start:ssh": "echo | source cdk_service.sh start service sshservice",
    "stop:ssh": "echo | source cdk_service.sh stop service sshservice",
    "start:oni": "echo | source cdk_service.sh start service oniservice",
    "stop:oni": "echo | source cdk_service.sh stop service oniservice",
    "sync:config": "source cdk_sync_config.sh",
    "sync:ocfl": "source cdk_sync_ocfl.sh.sh",
    "ssh": "source cdk_ssh.sh",
    "get:sshdns": "source cdk_get_output.sh sshLoadBalancer"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.78.0",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "aws-cdk": "1.78.0",
    "ts-node": "^8.1.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-ec2": "1.78.0",
    "@aws-cdk/aws-ecs": "1.78.0",
    "@aws-cdk/aws-ecs-patterns": "1.78.0",
    "@aws-cdk/aws-efs": "1.78.0",
    "@aws-cdk/core": "1.78.0",
    "source-map-support": "^0.5.16"
  }
}
